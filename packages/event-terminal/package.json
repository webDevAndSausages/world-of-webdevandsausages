{
	"name": "@webdevandsausages/event-terminal",
	"version": "0.6.1",
	"author": "Richard Van Camp",
	"license": "MIT",
	"description": "A component for display a WD&S event and executing, checking, or cancelling event resgistration",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/webDevAndSausages/world-of-webdevandsausages.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"svelte": "src/components/components.module.js",
	"module": "dist/index.min.mjs",
	"main": "dist/index.min.js",
	"devDependencies": {
		"@babel/core": "7.5.5",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-transform-runtime": "7.5.5",
		"@babel/preset-env": "7.5.5",
		"@fullhuman/postcss-purgecss": "1.2.0",
		"@testing-library/jest-dom": "4.1.0",
		"@testing-library/svelte": "1.8.0",
		"@types/jest": "24.0.18",
		"autoprefixer": "9.6.1",
		"babel-jest": "24.9.0",
		"caniuse-lite": "1.0.30001023",
		"compression": "1.7.4",
		"corejs": "1.0.0",
		"cors": "2.8.5",
		"cssnano": "4.1.10",
		"eslint": "6.2.2",
		"eslint-config-prettier": "6.1.0",
		"eslint-plugin-jest": "22.15.2",
		"eslint-plugin-svelte3": "2.7.3",
		"express": "4.17.1",
		"http-proxy-middleware": "0.19.1",
		"husky": "3.0.4",
		"jest": "24.9.0",
		"jest-transform-svelte": "2.0.2",
		"lint-staged": "9.2.4",
		"npm-run-all": "4.1.5",
		"polka": "0.5.2",
		"postcss": "7.0.36",
		"postcss-import": "12.0.1",
		"prettier": "1.18.2",
		"prettier-plugin-svelte": "0.7.0",
		"rollup": "1.20.2",
		"rollup-plugin-babel": "4.3.3",
		"rollup-plugin-commonjs": "10.0.2",
		"rollup-plugin-livereload": "1.0.1",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-postcss": "2.0.3",
		"rollup-plugin-replace": "2.2.0",
		"rollup-plugin-svelte": "5.1.0",
		"rollup-plugin-terser": "5.1.1",
		"sirv": "0.4.2",
		"svelte": "3.18.1",
		"svelte-preprocess": "2.15.1",
		"tailwindcss": "1.1.2",
		"tailwindcss-elevation": "0.3.2",
		"tinycolor2": "1.4.1"
	},
	"scripts": {
		"lint": "eslint --color ./src/**/*.js",
		"test": "jest --verbose",
		"autobuild": "rollup -c -w",
		"start:dev": "node devServer.js",
		"dev": "run-p start:dev autobuild",
		"build": "rollup -c",
		"prepublishOnly": "npm run build",
		"format-svelte": "prettier --write --plugin-search-dir=. ./src/**/*.svelte",
		"format-js": "prettier --config ./.prettierrc --write ./src/**/*.js",
		"format": "npm --scripts-prepend-node-path=true run format-svelte && npm run format-js",
		"release": "npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push origin master && git push --tags && npm publish"
	},
	"browserslist": "> 0.25%, not dead",
	"keywords": [
		"svelte"
	],
	"files": [
		"src/components",
		"dist/index.min.mjs",
		"dist/index.min.js"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js, svelte}": [
			"eslint --fix",
			"git add"
		],
		"*.{html, css, scss, stylus, js, ts, json, yml, md}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"classcat": "4.0.2",
		"daggy": "1.4.0",
		"date-fns": "2.9.0",
		"date-fns-tz": "1.0.9",
		"immer": "5.3.2",
		"ky": "0.13.0",
		"ramda": "0.26.1",
		"simplebar": "4.3.0-alpha.0",
		"smoothscroll-polyfill": "0.4.4",
		"viewprt": "4.0.0"
	},
	"bugs": {
		"url": "https://github.com/webDevAndSausages/world-of-webdevandsausages/issues"
	},
	"homepage": "https://github.com/webDevAndSausages/world-of-webdevandsausages#readme",
	"directories": {
		"test": "tests"
	}
}
